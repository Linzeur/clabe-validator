{
   "name": "clabe-validator",
   "version": "1.3.3",
   "description": "JavaScript library to analyze or create a CLABE number for a Mexican bank account",
   "license": "MIT",
   "main": "dist/clabe.js",
   "browser": "dist/clabe.min.js",
   "repository": "github:center-key/clabe-validator",
   "homepage": "https://github.com/center-key/clabe-validator",
   "bugs": "https://github.com/center-key/clabe-validator/issues",
   "docs": "https://centerkey.com/clabe",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "bank",
      "bank-account",
      "clabe",
      "clabe-number",
      "clabe-validator",
      "financial",
      "mexico",
      "javascript",
      "validator"
   ],
   "jshintConfig": {
      "esversion": 6,
      "strict": "implied",
      "undef": true,
      "unused": true,
      "browser": true,
      "mocha": true,
      "node": true
   },
   "scripts": {
      "pretest": "jshint *.js && gulp lint-html && gulp build-dist",
      "test": "mocha spec.js && env specMode=minified mocha spec.js"
   },
   "engines": {
      "node": ">=9.9.0"
   },
   "devDependencies": {
      "@babel/core": "~7.2",
      "@babel/preset-env": "~7.3",
      "@fortawesome/fontawesome-free": "~5.7",
      "babel-minify": "~0.5",
      "dna.js": "~1.5",
      "gulp": "~4.0",
      "gulp-append-prepend": "~1.0",
      "gulp-babel": "~8.0",
      "gulp-header": "~2.0",
      "gulp-htmlhint": "~2.2",
      "gulp-rename": "~1.4",
      "gulp-replace": "~1.0",
      "gulp-size": "~3.0",
      "gulp-w3c-html-validator": "~1.4",
      "jquery": "~3.3",
      "jshint": "~2.10",
      "mocha": "~5.2",
      "web-ignition": "~1.1"
   }
}
